.my-header{
  width: 100%;
  height: 250rpx;
  display: flex;
  align-items: center;
}
.my-bg{
  position: absolute;
  width: 100%;
  height: 250rpx;
}
.header-wrapper{
  z-index: 1;
  box-sizing: border-box;
  padding: 0 20rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.open-btn{
  display: flex;
  align-items: center;
}
.avatar-wrapper{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  overflow: hidden;
}
.nickname{
  color: #FFF;
  font-weight: 600;
  margin-left: 20rpx;
}
.vip{
  width: 180rpx;
  height: 60rpx;
  border-radius: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #6e6d6b;
  font-size: 26rpx;
  background-image: linear-gradient(180deg,#FFE4C4 0,#DEB887 100%);
}
.vip image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.my-service{
  margin-top: 20rpx;
}
.service{
  font-size: 28rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
}