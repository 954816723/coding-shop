.create-address{
  width: 100%;
  height: 100vh;
}
.default .van-checkbox{
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
  padding-top: 40rpx;
  padding-right: 40rpx;
  font-size: 26rpx;
}
.submit-btn{
  position: absolute;
  width: 100%;
  height: 140rpx;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.sub-btn button{
  border-radius: 20rpx;
  width: 500rpx;
  height: 80rpx;
}